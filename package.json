{
    "name": "slippi-scripts",
    "version": "1.0.0",
    "description": "The CLI Slippi script to rule them all",
    "main": "src/getGames.js",
    "dependencies": {
        "chalk": "^3.0.0",
        "chokidar": "^3.3.1",
        "electron": "^10.1.2",
        "elm": "^0.19.1-3",
        "elm-live": "^4.0.2",
        "lodash": "^4.17.19",
        "multispinner": "^0.2.1",
        "node-wav-player": "^0.1.0",
        "obs-websocket-js": "^4.0.1",
        "ora": "^4.0.3",
        "slp-parser-js": "^4.1.0",
        "yargs": "^15.1.0"
    },
    "devDependencies": {
        "chokidar-cli": "^2.1.0"
    },
    "scripts": {
        "play": "node src/playback play -c ../config.json",
        "record": "node src/playback record -c ../config.json",
        "test": "node src/getGames.js",
        "elm": "elm make gui/Main.elm --output index.js --optimize || true",
        "elm:watch": "chokidar '**/*.elm' -c 'npm run elm'",
        "start": "electron main.js",
        "watch": "npm run start & npm run elm:watch & wait"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/IvantheTricourne/slippi-scripts.git"
    },
    "keywords": [
        "Slippi",
        "SSBM",
        "JavaScript"
    ],
    "author": "Carl Factora",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/IvantheTricourne/slippi-scripts/issues"
    },
    "homepage": "https://github.com/IvantheTricourne/slippi-scripts#readme"
}
